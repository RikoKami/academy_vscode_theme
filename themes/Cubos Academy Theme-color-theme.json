{
    "name": "Cubos Academy Theme",
    "type": "dark",

    "workbench.colorCustomizations": {
        "activityBar.foreground": "#FFF",
        "statusBar.foreground": "#FFF",
    },

    "colors": {
        "editor.background": "#222222",
        "editorCursor.foreground": "#F8F8F0",
        "editor.foreground": "#fff",
        "editorWhitespace.foreground": "#f8f8f228",
        "editor.lineHighlightBackground": "#e5007a44",
        "editor.selectionBackground": "#e5007a44",
    },
    "tokenColors": [{
            "settings": {
                "foreground": "#F8F8F2"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#7a7fa0"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#ffb6dd"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#FFD84F"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#AE81FF"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#AE81FF"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#66D9EF"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#FF2499"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FF2499"
            }
        },
        {
            "name": "Class name",
            "scope": "entity.name.class",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#AE81FF"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#AE81FF"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FFD84F"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FFD84F"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#FF2499",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#1F91CC"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#66D9EF"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#66D9EF"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FF2499"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "background": "#FF2499",
                "fontStyle": "",
                "foreground": "#F8F8F0"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "background": "#AE81FF",
                "foreground": "#F8F8F0"
            }
        }
    ],

}